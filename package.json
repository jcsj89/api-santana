        "eslint-config-prettier": "^8.5.0",
        "express": "^4.18.2",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.1"
    },
    "dependencies": {
        "rimraf": "^3.0.2"
